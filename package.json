{
  "name": "vibrational-medicine-app",
  "version": "1.0.0",
  "description": "A comprehensive application for Vibrational Medicine practitioners",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace=backend"
  },
  "keywords": [
    "vibrational-medicine",
    "holistic-health",
    "practitioner-tools"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3"
  }
}